# Example environment variables for development and CI
NODE_ENV=development
PORT=3000
DB_DIALECT=sqlite
DB_STORAGE=./tmp/dev.sqlite
ALLOWED_ORIGINS=http://localhost:3000
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
# Firebase (optional) - add real values for production
FIREBASE_PROJECT_ID=
FIREBASE_PRIVATE_KEY=
FIREBASE_CLIENT_EMAIL=

# JWT and auth
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=7d

# Logging
LOG_LEVEL=info
